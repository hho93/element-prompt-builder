{
  "name": "element-prompt-builder",
  "version": "0.1.12",
  "description": "React component library for inspecting DOM elements and generating AI prompts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "author": "hho93",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "react",
    "dom",
    "inspector",
    "element",
    "highlighter",
    "selector",
    "prompt",
    "ai",
    "context-builder"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
