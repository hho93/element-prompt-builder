  },
  "dependencies": {},
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "eslint": "^9",
    "tsup": "^8.0.1",
    "typescript": "^5"
  },
  "publishConfig": {
    "access": "public"
  }
}
